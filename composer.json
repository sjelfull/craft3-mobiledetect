{
    "name": "superbig/craft3-mobiledetect",
    "description": "Use Mobile_Detect for detecting mobile devices (including tablets)",
    "type": "craft-plugin",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "mobiledetect"
    ],
    "support": {
        "docs": "https://github.com/sjelfull/craft3-mobiledetect/blob/craft5/README.md",
        "issues": "https://github.com/sjelfull/craft3-mobiledetect/issues"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Superbig",
            "homepage": "https://superbig.co"
        }
    ],
    "require": {
        "php": "^8.2.0",
        "craftcms/cms": "^5.5",
        "mobiledetect/mobiledetectlib": "^4.8"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main",
        "craftcms/rector": "dev-main",
        "markhuot/craft-pest-core": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "superbig\\mobiledetect\\": "src/"
        }
    },
    "scripts": {
        "phpstan": "phpstan --ansi --memory-limit=1G",
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --fix --ansi"
    },
    "config": {
        "allow-plugins": {
            "craftcms/plugin-installer": true,
            "yiisoft/yii2-composer": true,
            "pestphp/pest-plugin": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "name": "MobileDetect",
        "handle": "mobile-detect",
        "hasCpSettings": false,
        "hasCpSection": false,
        "class": "superbig\\mobiledetect\\MobileDetect"
    }
}
